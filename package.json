{
  "name": "sensing-flow-backend",
  "version": "0.0.1",
  "scripts": {
    "build": "tsx ./build.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "VITE_START=true && nest start --watch",
    "debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:cov": "vitest --coverage",
    "deploy:dev": "pnpm build && cdk deploy --all --profile sensing-flow-dev --region ap-northeast-1 --require-approval never --context env=dev",
    "deploy:stg": "pnpm build && cdk deploy --all --profile sensing-flow-stg --region ap-northeast-1 --require-approval never --context env=stg",
    "deploy:prod": "pnpm build && cdk deploy --all --profile sensing-flow-prod --region ap-northeast-1 --require-approval never --context env=prod"
  },
  "dependencies": {
    "@codegenie/serverless-express": "^4.17.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.1.10",
    "@nestjs/swagger": "^11.2.3",
    "body-parser": "^2.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "vite-plugin-node": "^7.0.0"
  },
  "devDependencies": {
    "@anatine/esbuild-decorators": "^0.2.19",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/microservices": "^11.1.10",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/aws-lambda": "^8.10.159",
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "aws-cdk": "^2.1100.1",
    "aws-cdk-lib": "^2.233.0",
    "aws-lambda": "^1.0.7",
    "esbuild": "^0.27.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "prettier": "^3.4.2",
    "serverless-offline": "^14.4.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
